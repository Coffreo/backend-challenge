{
    "repositories": [
        {
            "type": "path",
            "url": "../../packages/*",
            "options": {
                "symlink": false
            }
        }
    ],
    "require": {
        "internals/rabbitmq": "*",
        "monolog/monolog": "^3.5",
        "vlucas/phpdotenv": "^5.6",
        "psr/log": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "config": {
        "preferred-install": {
            "internals/*": "dist"
        }
    }
}