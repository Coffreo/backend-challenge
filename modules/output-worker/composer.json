{
  "repositories": [
    {
      "type": "path",
      "url": "../../packages/*",
      "options": {
        "symlink": false
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests"
    }
  },
  "config": {
    "preferred-install": {
      "internals/*": "dist"
    }
  },
  "require": {
    "internals/rabbitmq": "*",
    "monolog/monolog": "^3.5",
    "psr/log": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}